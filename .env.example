# .env.example - copy to .env and fill values
CLIENT_ID=your_linkedin_client_id
CLIENT_SECRET=your_linkedin_client_secret
REDIRECT_URI=http://localhost:8000/callback
# If you already have an access token you can paste it here; otherwise use oauth_server.py
ACCESS_TOKEN=
# optional: set author urn like urn:li:person:XXXXXXXX (if blank script will call /me)
AUTHOR_URN=
# scopes example: r_liteprofile r_emailaddress w_member_social offline_access
SCOPES=w_member_social
